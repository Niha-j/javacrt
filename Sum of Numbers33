import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int x=s.nextInt();
        int sum=x;
        while(x!=-1)
        {
            x=s.nextInt();
            sum+=x;
            
        }
        sum=sum+1;
        System.out.println(sum);
}
}
